{
  "name": "hexoscape",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "server": "node -r esm server.js",
    "devstart": "VITE_WITH_SEPARATE_SERVER=1 npm run dev",
    "devserver": "node -r esm devserver.js",
    "compile-game-files": "tsc ./src/game/*.ts ./src/game/hexxaform/*.ts --outDir ./server --downlevelIteration true --skipLibCheck true",
    "build": "npm run compile-game-files && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write 'src/**/*.js' '**/*.json' 'src/**/*.ts' 'src/**/*.tsx' 'server.js' 'devserver.js'",
    "preview": "vite preview"
  },
  "dependencies": {
    "@mui/material": "^6.1.0",
    "@mui/styled-engine-sc": "^6.1.0",
    "@react-three/drei": "^9.111.2",
    "@react-three/fiber": "^8.17.5",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "boardgame.io": "^0.50.2",
    "esm": "^3.2.25",
    "framer-motion": "^11.3.28",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-hot-toast": "^2.4.1",
    "react-hotkeys-hook": "^4.5.1",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.26.0",
    "styled-components": "^6.1.13",
    "three": "^0.167.1"
  },
  "devDependencies": {
    "prettier": "^3.3.3",
    "@types/jest": "^29.5.12",
    "@types/koa-static": "^4.0.4",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-helmet": "^6.1.11",
    "@types/styled-components": "^5.1.34",
    "@types/three": "^0.167.1",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "typescript": "^5.2.2",
    "vite": "^5.3.4"
  }
}
